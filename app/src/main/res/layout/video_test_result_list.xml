<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    >

    <Button
        android:id="@+id/test_list_btn1"
        android:layout_width="340dp"
        android:layout_height="110dp"

        android:layout_marginTop="20dip"
        android:text="결과보기"
        android:textSize="28sp"
        android:textColor="@color/white"
        android:background="@drawable/btn_video_test_restlt"

        />

<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/btn_video_test_restlt_center"
    android:orientation="vertical"
    android:gravity="center"
    android:padding="10dp"
    android:layout_marginTop="20dp"
    >

    <ImageButton
        android:id="@+id/btn_video_test_result_parent"
        android:layout_width="320dp"
        android:layout_height="100dp"

        android:contentDescription="학부모"
        android:background="@drawable/btn_parent"
        />

    <ImageButton
        android:id="@+id/btn_video_test_result_child"
        android:layout_width="320dp"
        android:layout_height="100dp"
        android:layout_marginTop="10dip"
        android:contentDescription="학생"
        android:background="@drawable/btn_child"
         />
</LinearLayout>

</LinearLayout>
