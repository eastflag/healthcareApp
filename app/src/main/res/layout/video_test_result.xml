<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/video_result_bg"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_horizontal"
    android:orientation="vertical">

    <ImageButton
        android:id="@+id/test_list_btn1"
        android:layout_width="340dp"
        android:layout_height="110dp"
        android:layout_marginTop="20dip"
        android:background="@drawable/btn_self"
        android:contentDescription="검사 아이콘"

        />


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:background="@drawable/btn_video_test_restlt"
        android:gravity="center"
        android:orientation="vertical"
        android:padding="10dp">

        <ScrollView
            android:id="@+id/video_result_scroll"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbars="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <TextView
                    android:id="@+id/video_result_text"
                    style="@style/exercise_text"
                    android:layout_width="320dp"
                    android:layout_height="420dp"
                    android:gravity="left"
                    android:paddingTop="5dp"
                    android:paddingLeft="10dp"
                    />
            </LinearLayout>

        </ScrollView>
    </LinearLayout>

</LinearLayout>
